{
  "name": "repo-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint:next": "next lint",
    "eslint": "eslint",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --cache --cache-location=./.next/cache/eslint/.standalone-cache",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix --cache --cache-location=./.next/cache/eslint/.standalone-cache",
    "format": "prettier \"**/*.{js,cjs,mjs,ts,tsx,md,json,yml}\" --write --cache --cache-location=./.next/cache/prettier/.cache"
  },
  "dependencies": {
    "next": "14.2.5",
    "next-intl": "^3.17.3",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-variants": "^0.2.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@stylistic/eslint-plugin": "^2.6.4",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unicorn": "^55.0.0",
    "lefthook": "^1.7.14",
    "lint-staged": "^15.2.9",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
